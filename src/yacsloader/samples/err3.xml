<proc>
  <!--Schema for testing errors : consistency errors -->

    <!--types -->
    <!--nodes -->
    <inline name="n" >
        <script>
            <code>select=3</code>
        </script>
        <outport name="select" type="int"/>
    </inline>
    <inline name="n2" >
        <script>
            <code>print p1</code>
        </script>
        <inport name="p1" type="double"/>
        <outport name="p1" type="double"/>
    </inline>
    <switch name="b1">
      <case id="1">
        <inline name="n2" >
          <script><code>print p1</code></script>
          <inport name="p1" type="double"/>
