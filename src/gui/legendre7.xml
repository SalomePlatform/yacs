   <inline name="poly_7">
      <script><code><![CDATA[Pn=(429*x**7 -693*x**5+315*x**3 -35*x)/16
print "reference P7=",Pn]]></code></script>
      <inport name="x" type="Double"/>
      <outport name="Pn" type="Double"/>
   </inline>
   <inline name="init">
      <script><code><![CDATA[x=0.5
nsteps=3]]></code></script>
      <outport name="x" type="Double"/>
      <outport name="nsteps" type="Int"/>
   </inline>
   <bloc name="Legendre">
      <forloop name="loopIter" nsteps="3">
         <bloc name="deuxIter">
            <inline name="iter1">
               <script><code><![CDATA[Pn = ((2*n-1)*x*Pnm1 -(n-1)*Pnm2)/n
print "n=",n," Pn=",Pn
n=n+1]]></code></script>
               <inport name="Pnm1" type="Double"/>
               <inport name="Pnm2" type="Double"/>
               <inport name="x" type="Double"/>
               <inport name="n" type="Int"/>
               <outport name="Pn" type="Double"/>
               <outport name="n" type="Int"/>
            </inline>
            <inline name="iter2">
               <script><code><![CDATA[Pn = ((2*n-1)*x*Pnm1 -(n-1)*Pnm2)/n
print "n=",n," Pn=",Pn
n=n+1]]></code></script>
               <inport name="Pnm1" type="Double"/>
               <inport name="Pnm2" type="Double"/>
               <inport name="x" type="Double"/>
               <inport name="n" type="Int"/>
               <outport name="Pn" type="Double"/>
               <outport name="n" type="Int"/>
            </inline>
            <control> <fromnode>iter1</fromnode> <tonode>iter2</tonode> </control>
            <datalink control="false">
               <fromnode>iter1</fromnode> <fromport>Pn</fromport>
               <tonode>iter2</tonode> <toport>Pnm1</toport>
            </datalink>
            <datalink control="false">
               <fromnode>iter1</fromnode> <fromport>n</fromport>
               <tonode>iter2</tonode> <toport>n</toport>
            </datalink>
            <datalink control="false">
               <fromnode>iter2</fromnode> <fromport>Pn</fromport>
               <tonode>iter1</tonode> <toport>Pnm1</toport>
            </datalink>
            <datalink control="false">
               <fromnode>iter2</fromnode> <fromport>n</fromport>
               <tonode>iter1</tonode> <toport>n</toport>
            </datalink>
         </bloc>
         <datalink control="false">
            <fromnode>deuxIter.iter2</fromnode> <fromport>Pn</fromport>
            <tonode>deuxIter.iter2</tonode> <toport>Pnm2</toport>
         </datalink>
         <datalink control="false">
            <fromnode>deuxIter.iter1</fromnode> <fromport>Pn</fromport>
            <tonode>deuxIter.iter1</tonode> <toport>Pnm2</toport>
         </datalink>
      </forloop>
      <inline name="iter_1">
